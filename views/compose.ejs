<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label><br>
    <input class="form-control" type="text" name="postTitle" />
  </div>
  <div class="form-group">
    <label for="postTextArea">Post</label>
    <textarea
      class="form-control"
      name="postTextArea"
      rows="3"
    ></textarea>
  </div>
  <div class="form-group">
    <button class="btn btn-custom" type="submit" name="submitButton">Publish</button>
  </div>
</form>
<%- include("partials/footer") -%>
